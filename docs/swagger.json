{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "basePath": "/api/v1",
    "paths": {
        "/endpoints/add": {
            "post": {
                "description": "新增/修改路由配置（未携带id信息为新增）",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "endpoints"
                ],
                "summary": "新增/修改路由配置",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/endpoints/delete": {
            "post": {
                "description": "删除路由配置",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "endpoints"
                ],
                "summary": "删除路由配置",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "网关配置的id",
                        "name": "id",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/endpoints/detail": {
            "post": {
                "description": "获取路由配置详情",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "endpoints"
                ],
                "summary": "获取路由配置详情",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "网关配置的id",
                        "name": "id",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/endpoints/list": {
            "post": {
                "description": "获取所有路由配置",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "endpoints"
                ],
                "summary": "获取所有路由配置",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "页码数",
                        "name": "page_num",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 20,
                        "description": "分页尺寸",
                        "name": "page_size",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/oauth/login/password": {
            "post": {
                "description": "账号密码登录",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "Oauth"
                ],
                "summary": "账号密码登录",
                "operationId": "OauthLoginPassword",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "用户名",
                        "name": "user_name",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "密码",
                        "name": "password",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/register/admin": {
            "post": {
                "description": "注册管理员用户",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "register"
                ],
                "summary": "注册管理员用户",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "用户名",
                        "name": "user_name",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "密码",
                        "name": "password",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/register/tenant": {
            "post": {
                "description": "注册租户",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "register"
                ],
                "summary": "注册租户",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "用户名",
                        "name": "user_name",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "密码",
                        "name": "password",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/version": {
            "get": {
                "description": "获取当前版本",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "tags": [
                    "system"
                ],
                "summary": "获取当前版本",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}